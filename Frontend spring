ğŸ”¹ Modify Button Click (Validation + Data Prep)

var mineModifySave = document.getElementById("modifyDetailsMine");

mineModifySave.addEventListener("click", function () {

    let mineLength = document.getElementById("modifyMineLength").value;
    let mineDia = document.getElementById("modifyMineDia").value;
    let noOfMines = document.getElementById("modifyNoOfMines").value;

    if (aopFlag === false) {
        swal.fire("AOP is not defined !!!");
        return;
    }

    if (mineLength === "" || mineDia === "" || noOfMines === "") {
        swal.fire("Incomplete Details !!!");
        return;
    }

    if (mineLength <= 0 || isNaN(mineLength)) {
        swal.fire("Length value is incorrect !!!");
        return;
    }

    if (mineDia <= 0 || isNaN(mineDia)) {
        swal.fire("Diameter value is incorrect !!!");
        return;
    }

    let selectedIndex = document.getElementById("selectMine").selectedIndex;
    let obj = mines[selectedIndex - 1];
    let id = parseInt(obj.id);

    updateMineDetails(obj, id, mineLength, mineDia, noOfMines);
});


