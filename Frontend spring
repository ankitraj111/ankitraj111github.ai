ğŸ”¹ AJAX Call (Sending to Backend)

function updateMineDetails(obj, id, mineLength, mineDia, noOfMines) {

    let newFormData = {};

    newFormData.id = id;
    newFormData.mineName = obj.mineName;
    newFormData.mineLength = mineLength;
    newFormData.mineDia = mineDia;
    newFormData.noOfMines = noOfMines;
    newFormData.latFrom = obj.latFrom;
    newFormData.latTo = obj.latTo;
    newFormData.longFrom = obj.longFrom;
    newFormData.longTo = obj.longTo;
    newFormData.scenarioId = scenarioId;
    newFormData.mineId = obj.mineId;
    newFormData.minePoints = obj.minePoints;

    mines[selectedIndex - 1] = newFormData;

    $.ajax({
        url: "mines",
        type: "POST",
        data: JSON.stringify(newFormData),
        dataType: "json",
        contentType: "application/json",
        success: function (response) {
            swal.fire("Details successfully updated");
        },
        error: function (jqXHR, textStatus, errorThrown) {
            swal.fire("Error in updating laying mines object");
        }
    });
}


